fyContent: \"center\", marginTop: \"20vh\" }}\n    >\n      <form className=\"mx-auto w-50\">\n        <p\n          style={{\n            display: \"flex\",\n            justifyContent: \"center\",\n            alignItems: \"center\",\n          }}\n        >\n          Enter Your Registred E-mail\n        </p>\n\n        <div className=\"form-group\" ref={mailRef}>\n          <input\n            type=\"email\"\n            className=\"form-control\"\n            placeholder=\"Email\"\n            name=\"email\"\n            value={email}\n            onChange={(e) => handleChange(e)}\n          />\n          <br />\n\n          <small style={{ color: \"red\", marginLeft: \"5vw\" }}>\n            {errors.email}\n          </small>\n          {errors.email === \"Enter a valid Email\" && (\n            <i\n              className=\"fa fa-exclamation-circle\"\n              style={{ color: \"red\" }}\n            ></i>\n          )}\n          {errors.email === true && (\n            <i className=\"fa fa-check-square-o\" style={{ color: \"green\" }}></i>\n          )}\n        </div>\n        <div className=\"d-flex\">\n          <button\n            onClick={(e) => formValidate(e)}\n            className=\"btn btn-primary\"\n            data-toggle=\"modal\"\n            data-target=\"#staticBackdrop\"\n          >\n            Submit\n          </button>\n        </div>\n      </form>\n    </div>\n  );\n};\n\nexport default Forget;\n","/home/jmh/Desktop/ReactJS/startupbundle_reactjs/src/pages/dashboard/Dashboard.jsx",[],"/home/jmh/Desktop/ReactJS/startupbundle_reactjs/src/components/Loader.jsx",[],"/home/jmh/Desktop/ReactJS/startupbundle_reactjs/src/redux/configStore.js",[],"/home/jmh/Desktop/ReactJS/startupbundle_reactjs/src/pages/gallary/styles.jsx",[],"/home/jmh/Desktop/ReactJS/startupbundle_reactjs/src/components/header/Header.jsx",[],"/home/jmh/Desktop/ReactJS/startupbundle_reactjs/src/utilities/services/newServices.js",[],"/home/jmh/Desktop/ReactJS/startupbund