refresh, setRefresh] = useState(true)\n  const loaderMargin = {\n    marginTop: \"50vh\"\n  }\n\n  const getUser = () => {\n    const successActions = (userData) => {\n      setUserData(userData);\n      setLoading(false);\n      var fullname =\n        setFullName(`${userData.firstName} ${userData.lastName}`)\n      var avatar = userData.avatar\n      setAvatar(userData.avatar);\n\n\n    };\n\n    const errActions = (err) => {\n      alert(err);\n      history.push(\"/login\");\n    };\n\n    const token = localStorage.getItem(\"token\");\n    const id = localStorage.getItem(\"id\");\n    get(`${GET_USER}${id}`, successActions, errActions, token);\n  };\n\n  useEffect(() => {\n    getUser();\n  }, [refresh]);\n\n  const refreshFn = () => {\n    setRefresh(!refresh)\n  }\n\n\n\n  return (\n    <>\n      {\n        loading ? <Loader loaderMargin={loaderMargin}\n        /> : <div style={{ marginTop: \"15vh\" }}>\n            <Page\n              className={classes.root}\n              title=\"Account\"\n            >\n              <Container maxWidth=\"lg\">\n                <Grid\n                  container\n                  spacing={3}\n                >\n                  <Grid\n                    item\n                    lg={4}\n                    md={6}\n                    xs={12}\n                  >\n                    <Profile fullName={fullName} avt={avatar} refreshFn={refreshFn} />\n                  </Grid>\n                  <Grid\n                    item\n                    lg={8}\n                    md={6}\n                    xs={12}\n                  >\n                    <ProfileDetails fName={userData.firstName} lName={userData.lastName} emailAdd={userData.email} phone={userData.phoneNumber} />\n                  </Grid>\n                </Grid>\n              </Container>\n            </Page>\n          </div>\n      }\n    </>\n  );\n};\n\nexport default Account;\n","/home/jmh/Desktop/ReactJS/startupbundle_reactjs/src/pages/otp/Otp.jsx